{
  "name": "client",
  "version": "0.1.0",
  "description": "Client Web App for Slack Viz",
  "main": "index.js",
  "contributors": [
    {
      "name": "Oleksii Rudenko",
      "email": "oleksii.rudenko@moovel.com"
    },
    {
      "name": "Flore De Crombrugghe",
      "email": "flore.crombrugghe@moovel.com"
    },
    {
      "name": "Josefine Hartung",
      "email": "josefine.hartung@moovel.com"
    },
    {
      "name": "Benedikt Gro√ü",
      "email": "benedikt.gross@moovel.com"
    }
  ],
  "jspm": {
    "name": "client",
    "main": "client.js",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "babel-types": "npm:babel-types@6.9.0",
      "css": "github:systemjs/plugin-css@^0.1.23",
      "flexboxgrid": "npm:flexboxgrid@^6.3.0",
      "immutable": "npm:immutable@^3.8.1",
      "lodash": "npm:lodash@^4.13.1",
      "moment": "npm:moment@^2.13.0",
      "node-emoji": "npm:node-emoji@^1.3.0",
      "normalize.css": "github:necolas/normalize.css@^4.1.1",
      "react-addons-update": "npm:react-addons-update@^15.1.0",
      "react-button-group": "npm:react-button-group@^1.0.2",
      "react-date-range": "npm:react-date-range@^0.2.4",
      "react-modal": "npm:react-modal@^1.3.0",
      "react-progress-2": "npm:react-progress-2@^4.2.1",
      "react-router": "npm:react-router@^2.4.1",
      "react-virtualized": "npm:react-virtualized@^7.3.3",
      "react-vis": "npm:react-vis@^0.3.4",
      "scss": "github:mobilexag/plugin-sass@^0.4.5",
      "whatwg-fetch": "npm:whatwg-fetch@^1.0.0"
    },
    "devDependencies": {
      "babel-plugin-transform-react-jsx": "npm:babel-plugin-transform-react-jsx@^6.8.0",
      "core-js": "npm:core-js@^2.4.0",
      "net": "github:jspm/nodelibs-net@^0.2.0-alpha",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.10",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.6.0",
      "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "d3": "npm:d3@^3.5.17",
      "domain": "github:jspm/nodelibs-domain@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "react": "npm:react@^15.1.0",
      "react-addons-shallow-compare": "npm:react-addons-shallow-compare@^15.1.0",
      "react-dom": "npm:react-dom@^15.0.2",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "timers": "github:jspm/nodelibs-timers@^0.2.0-alpha",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zlib": "github:jspm/nodelibs-zlib@^0.2.0-alpha"
    },
    "overrides": {
      "npm:babel-types@6.9.0": {
        "meta": {
          "lib/index.js": {
            "deps": [
              "./definitions/init.js"
            ],
            "cjsDeferDepsExecute": true
          },
          "lib/definitions/init.js": {
            "deps": [
              "./index",
              "./core",
              "./es2015",
              "./flow",
              "./jsx",
              "./misc",
              "./experimental"
            ],
            "cjsDeferDepsExecute": true
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:d3@3.5.17": {
        "meta": {
          "d3.js": {
            "exports": "d3",
            "format": "global"
          }
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.13.1": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:moment@2.13.0": {
        "jspmNodeConversion": false
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:sass.js@0.9.10": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist; mkdir dist; cp index.prod.html dist/index.html; jspm bundle client.js app-bundle.js --minify --skip-source-maps; cp app-bundle.js dist/; cp jspm_packages/system.js dist/system.js; cp jspm.browser.prod.js dist/jspm.browser.js; cp jspm.config.js dist/jspm.config.js; cp -R fonts dist/fonts; cp -R images dist/images"
  },
  "keywords": [],
  "author": "Moovel GmbH",
  "license": "LGPLv2.1",
  "devDependencies": {
    "jspm": "^0.17.0-beta.14"
  }
}
